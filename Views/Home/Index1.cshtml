
@model Root1

<div class="text-center">
     <img src="../../css/Rickandmorty.png">
</div>
<br>


<div class="inside-body">
<div class="container text-center">     
<div class="row">
<br>
        @foreach (var persona in @Model.results)
        {
            <div class="col-md-4">
                <div class="card" style="width: 18rem; margin-bottom: 25px">
                    <div class="card-body">
                        <h5 class="card-title"> @persona.name </h5>
                        <p class="card-text"><b>Tipo:</b> @persona.type </p>
                        <p class="card-text"><b>Diemensión:</b> @persona.dimension </p>
                    </div>
                </div>
            </div>
        }

</div>
</div>
</div>


